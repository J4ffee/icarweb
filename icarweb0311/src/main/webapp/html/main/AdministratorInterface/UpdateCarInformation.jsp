<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
</head>
<body>
<table border="1" class="table table-striped" style="width: 100%">
		<tr>
			<td>车主姓名</td>
			<td><input id="username" type="text" size="2" class="form-control"
				style="width: 50%"></td>
		</tr>
		<tr>
			<td>车牌</td>
			<td><input id="carid" type="text" size="2" class="form-control"
				style="width: 50%"></td>
		</tr>
		<tr>
			<td>车型</td>
			<td><input id="type" type="text" size="2" class="form-control"
				style="width: 50%"></td>
		</tr>
		<tr>
			<td>车主电话</td>
			<td><input id="userphone" type="text" size="2" class="form-control"
				style="width: 50%"></td>
		</tr>
	</table>
</body>
</html>