<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="viewport" content="width=device-width, initial-scale=1">
<link href="../../../bootstrap/css/bootstrap.min.css" rel="stylesheet">
<link href="../../../css/base.css" rel="stylesheet">
<script src="../../../js/jquery.js"></script>
<script src="../../../bootstrap/js/bootstrap.min.js"></script>
</head>
<body>


	<table border="1" class="table table-striped" style="width: 100%">
		<tr>
			<td>车主姓名</td>
			<td><input id="username" type="text" size="2" class="form-control"
				style="width: 32%"></td>
		</tr>
		<tr>
			<td>车牌</td>
			<td><input id="carid" type="text" size="2" class="form-control"
				style="width: 32%"></td>
		</tr>
		<tr>
			<td>车型</td>
			<td><input id="type" type="text" size="2" class="form-control"
				style="width: 32%"></td>
		</tr>
		<tr>
			<td>车主电话</td>
			<td><input id="phone" type="text" size="2" class="form-control"
				style="width: 32%"></td>
		</tr>
	</table>


</body>
</html>